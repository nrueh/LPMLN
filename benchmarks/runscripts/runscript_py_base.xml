<runscript output="output/{APP}/{DOM}/{OPT}/{NAME}">

	<machine name="komputer" cpu="24x8xE5520@2.27GHz" memory="24GB"/>
	
	<config name="seq-general" template="programs/seq-general.sh"/>
  
	<system name="plingo" version="1" measures="clingo" config="seq-general">

		<setting name="{OPT}" cmdline="{OPT} {APP} {DOM} {EXTRA}" tag="basic" />


    </system>

	<seqjob name="seq-gen" timeout="1200" runs="1" script_mode="timeout" walltime="00:50:00" parallel="1"/>

	<benchmark name="plingo-benchmark">
		<folder path="../../dom/{DOM}/{APP}/instances"> 
		</folder> 
	</benchmark>
	
	<project name="plingo-project-py" job="seq-gen">
		<runtag machine="komputer" benchmark="plingo-benchmark" tag="basic"/>
	</project>


</runscript>

